{
  "name": "header-map",
  "jest": {
    "verbose": true,
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy"
    },
    "coverageThreshold": {
      "./client/src": {
        "branches": 20,
        "functions": 20,
        "lines": 20,
        "statements": 10
      }
    },
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!**/node_modules/**",
      "!**/package.json/**",
      "!**/package-lock.json/**",
      "!**/webpack.config.js/**"
    ],
    "setupTestFrameworkScriptFile": "./tests/setup.js",
    "unmockedModulePathPatterns": [
      "react",
      "enzyme",
      "jest-enzyme",
      "css-loader"
    ]
  },
  "description": "> Project description",
  "author": "",
  "license": "ISC",
  "engines": {
    "node": ">=6.13.0"
  },
  "scripts": {
    "pack": "webpack --mode production --watch",
    "seed": "node database/rawData.js",
    "start": "nodemon server/index.js",
    "test": "jest --coverage --verbose"
  },
  "dependencies": {
    "artillery": "^1.6.0-24",
    "eslint": "^5.5.0",
    "express-http-proxy": "^1.4.0",
    "morgan": "^1.9.1",
    "newrelic": "^4.9.0",
    "nodemon": "^1.18.4",
    "redis": "^2.8.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^7.0.0-0",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.2",
    "bluebird": "^3.5.2",
    "body-parser": "^1.18.3",
    "cors": "^2.8.4",
    "css-loader": "^1.0.0",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "eslint-config-hackreactor": "git://github.com/reactorcore/eslint-config-hackreactor.git",
    "express": "^4.16.3",
    "faker": "^4.1.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^23.6.0",
    "jest-enzyme": "^6.0.4",
    "jquery": "^3.3.1",
    "mongoose": "^5.2.13",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "regenerator-runtime": "^0.12.1",
    "style-loader": "^0.23.0",
    "underscore": "^1.9.1",
    "webpack": "^4.17.2",
    "webpack-cli": "^3.1.0"
  },
  "version": "1.0.0",
  "main": ".eslintrc.js",
  "directories": {
    "test": "tests"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/i-8it/header-map.git"
  },
  "bugs": {
    "url": "https://github.com/i-8it/header-map/issues"
  },
  "homepage": "https://github.com/i-8it/header-map#readme"
}
